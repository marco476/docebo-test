{
    "extends": [
        "airbnb-typescript",
        "plugin:jest/recommended",
        "plugin:jest/style",
        "prettier"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "no-plusplus": 0,
      "@typescript-eslint/camelcase": 0
